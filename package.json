{
  "name": "@iamxiyang/env-checker",
  "version": "0.1.0-beta.0",
  "description": "Check the environment running the current code  帮助检查运行当前代码的环境",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --ext .ts src/",
    "release": "bumpp --commit --push --tag",
    "test": "vitest"
  },
  "keywords": [
    "environment",
    "env-checker",
    "env-info",
    "env-detect"
  ],
  "bugs": {
    "url": "https://github.com/iamxiyang/env-checker/issues"
  },
  "homepage": "https://github.com/iamxiyang/env-checker#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "hexiyang",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "bumpp": "^9.1.0",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vitest": "^0.31.4"
  }
}
